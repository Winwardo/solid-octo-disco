    "gulp": "^3.9.1",
    "istanbul": "^0.4.2",
    "jscs": "^2.11.0",
    "mocha": "^2.4.5",
    "mocha-lcov-reporter": "^1.2.0",
    "nodemon": "^1.9.1",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.9.1"
