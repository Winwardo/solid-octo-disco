  "repository": {
    "type": "git",
    "url": "git+https://github.com/Winwardo/solid-octo-disco.git"
  },
  "scripts": {
    "test-quick": "cross-env NODE_ENV=test mocha ./src/**/*.spec.js --compilers js:babel-core/register",
    "test": "babel-node ./node_modules/istanbul/lib/cli cover node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf coverage",
    "lint": "jscs src --fix",
    "build": "gulp build",
    "watch": "gulp watch",
    "dev": "cross-env NODE_ENV=development gulp run",
    "start": "cross-env NODE_ENV=production node build/server.js"
  },
  "author": [
    "Topher Winward",
