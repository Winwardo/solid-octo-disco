{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
      "browser": true,
      "node": true,
      "mocha": true,
      "jquery": true
  },
  "rules": {
      "indent": [2, 2],
      "no-console": 0,
      "no-alert": 0,
      "comma-dangle": 0,
      "react/jsx-no-bind": 0,
      "react/prop-types": 0
  }
}
